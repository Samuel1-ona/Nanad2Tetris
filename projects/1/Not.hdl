/**
 * Not gate:
 * Implements a logical negation.
 * if in = 0 then out = 1,
 * if in = 1 then out = 0
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    Nand(a=in, b=in, out=out);
}

